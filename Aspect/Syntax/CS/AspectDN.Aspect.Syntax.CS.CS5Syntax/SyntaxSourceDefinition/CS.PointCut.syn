// pointcut declaration	
+pointcut-declaration:
	pointcut-type identifier  ':' pointcut-expression ';'

+pointcut-expression:
	expression

+pointcut-type:
	'assemblies' 
	'classes'  
	'interfaces'
	'methods'
	'fields'
	'properties'
	'events'
	'delegates'
	'structs'
	'exceptions'
	'constructors'
	'enums'

